{
  "name": "tailwind-bootstrap-grid",
  "version": "2.1.0",
  "description": "Tailwindcss plugin to generate Bootstrap flexbox grid system.",
  "keywords": [
    "bootstrap",
    "flexbox",
    "grid",
    "grid system",
    "layout",
    "rtl",
    "tailwindcss"
  ],
  "homepage": "https://github.com/buz-zard/tailwind-bootstrap-grid#readme",
  "bugs": {
    "url": "https://github.com/buz-zard/tailwind-bootstrap-grid/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buz-zard/tailwind-bootstrap-grid.git"
  },
  "license": "MIT",
  "author": "Karolis Å arapnickis",
  "main": "index.js",
  "scripts": {
    "build": "run-p build:*",
    "build:demo": "cross-env NODE_ENV=production webpack -p --config demo/webpack.config.js",
    "build:sample": "tailwind build sample/input.css -c sample/tailwind.js -o sample/output.css --no-autoprefixer",
    "clean": "rimraf sample/output.css build",
    "format": "prettier --write '**/*.*' && sort-package-json",
    "lint": "run-p lint:*",
    "lint:js": "eslint .",
    "lint:md": "markdownlint README.md",
    "qa": "run-p test lint",
    "release": "npm i && run-s clean build qa && standard-version",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --config demo/webpack.config.js",
    "tdd": "npm test -- --watch",
    "test": "jest tests",
    "try-demo": "run-s clean build:demo && serve build",
    "upgrade-interactive": "npx npm-check --update"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,json}": [
      "pretty-quick --staged"
    ],
    "package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "reduce-css-calc": "^2.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@k.sh/eslint-config": "^1.11.0",
    "@mdx-js/loader": "^0.20.3",
    "@mdx-js/tag": "^0.20.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "^2.1.0",
    "extract-css-chunks-webpack-plugin": "^4.3.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.0.0",
    "jest": "^24.7.1",
    "lint-staged": "^9.0.0",
    "markdownlint-cli": "^0.17.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.14",
    "postcss-loader": "^3.0.0",
    "pretty-quick": "^1.10.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.8.4",
    "react-live": "^1.12.0",
    "rimraf": "^2.6.3",
    "serve": "^11.0.0",
    "sort-package-json": "^1.22.1",
    "standard-version": "^6.0.1",
    "stylefmt": "^6.0.3",
    "tailwindcss": "^1.0.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "peerDependencies": {
    "tailwindcss": "^1 || ^1.0.0-beta"
  },
  "greenkeeper": {
    "ignore": [
      "@mdx-js/loader",
      "@mdx-js/tag",
      "react-live"
    ]
  }
}
