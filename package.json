{
  "name": "tailwind-bootstrap-grid",
  "version": "4.0.0",
  "description": "Tailwind CSS plugin to generate Bootstrap flexbox grid system.",
  "keywords": [
    "bootstrap",
    "flexbox",
    "grid",
    "grid system",
    "layout",
    "rtl",
    "tailwindcss"
  ],
  "homepage": "https://github.com/karolis-sh/tailwind-bootstrap-grid#readme",
  "bugs": {
    "url": "https://github.com/karolis-sh/tailwind-bootstrap-grid/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karolis-sh/tailwind-bootstrap-grid.git"
  },
  "license": "MIT",
  "author": "Karolis Å arapnickis",
  "main": "lib/index.js",
  "scripts": {
    "build": "run-p build:*",
    "build:demo": "cross-env NODE_ENV=production webpack --mode production --config demo/webpack.config.js",
    "build:sample": "tailwind build sample/input.css -c sample/tailwind.js -o sample/output.css --no-autoprefixer",
    "clean": "rimraf sample/output.css build",
    "format": "run-p format:*",
    "format:js": "prettier --write '**/*.*'",
    "format:package-json": "sort-package-json",
    "lint": "run-p lint:*",
    "lint:js": "eslint . --cache",
    "lint:md": "markdownlint-cli2 '**/*.md' '!node_modules' '!**/node_modules' '!**/CHANGELOG.md'",
    "qa": "run-p test lint",
    "release": "yarn && run-s clean build qa && standard-version --no-verify && git push --follow-tags --no-verify && npm publish",
    "dev": "webpack serve --mode development --config demo/webpack.config.js",
    "tdd": "npm test -- --watch",
    "test": "jest tests",
    "try-demo": "run-s clean build:demo && serve build",
    "upgrade-interactive": "npx npm-check --update",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.{md,css,json,yml}": "pretty-quick --staged",
    "package.json": "sort-package-json"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "joi": "^17.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@k.sh/eslint-config": "^1.21.0",
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "markdownlint-cli2": "^0.1.3",
    "mini-css-extract-plugin": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.15",
    "postcss-loader": "^5.3.0",
    "pretty-quick": "^3.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hot-loader": "^4.13.0",
    "react-live": "^2.2.3",
    "rimraf": "^3.0.0",
    "serve": "^11.3.2",
    "sort-package-json": "^1.50.0",
    "standard-version": "^9.3.0",
    "tailwindcss": "^2.0.3",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "peerDependencies": {
    "tailwindcss": "^2"
  }
}
